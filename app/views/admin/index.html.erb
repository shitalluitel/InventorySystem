<div class="row">
  <div class="col-md-12">
      <div class="col-md-4">
        <div class="panel panel-primary">
          <div class="panel-heading">
           <h3 class="panel-title"><i class="fa fa-user fa-fw">&nbsp;</i> Users</h3>
          </div>
          <div class="panel-body">
            <h3 class="text-center text-primary"><%= @user %></h3>
          </div>
          <div class="panel-footer">
            <div class="row">
              <div class="col-md-12">
                <a href="<%= users_view_path %>" class="btn btn-primary">View Users</a>
                <a href="<%= users_new_path %>" class="btn btn-success">Add Users</a>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="col-md-4">
        <div class="panel panel-primary">
          <div class="panel-heading">
            <h3 class="panel-title"><i class="fa fa-user fa-history fa-fw">&nbsp;</i>User Logs</h3>
          </div>
          <div class="panel-body">
            <h3 class="text-center text-primary"><%= @log %></h3>
          </div>
          <div class="panel-footer">
            <div class="row">
              <div class="col-md-12">
                <a href="<%= logs_path %>" class="btn btn-primary">View Users</a>
              </div>
            </div>
          </div>
        </div>
      </div>
  </div>
</div>