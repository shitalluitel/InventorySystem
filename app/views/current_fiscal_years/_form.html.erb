<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="well tt-modal">
      <%= form_for (@current_fiscal_year) do |f| %>
          <div class="form-horizontal">
            <div class="form-group">
              <label class="col-md-3 control-label">Fiscal Years</label>
              <div class="control-label col-md-9
">
                <%= f.collection_select :fiscal_year_id, FiscalYear.order(:name), :id, :name, {prompt: "Select Category"}, {class: "form-control"} %>
              </div>
            </div>

            <div class="form-group">
              <div class="col-md-offset-3 col-md-4">
                <%= f.submit "Submit", :class => "btn btn-primary" %>
              </div>
            </div>
          </div>
      <% end %>
    </div>
  </div>
</div>